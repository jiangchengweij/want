{
  "name": "want",
  "version": "1.0.1",
  "description": "基于UniApp的轻量、高效组件库",
  "scripts": {
    "dev": "node build/dev.mjs",
    "dev:h5": "uni -p h5 -c src/vite.config.js",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint --ext .js,.ts --fix --quiet ./packages",
    "lint:style": "stylelint --fix \"./packages/**/*.scss\"",
    "build:lib": "npx gulp -f build/compiler.js --series buildLib",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "prepare": "husky install",
    "pre-commit": "npm run lint"
  },
  "lint-staged": {
    "./packages/**/*.{js,vue,ts}": [
      "npm run lint:script",
      "git add ."
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-app": "3.0.0-3090820231124001",
    "@dcloudio/uni-cli-shared": "3.0.0-3090820231124001",
    "@dcloudio/uni-components": "3.0.0-3090820231124001",
    "@dcloudio/uni-h5": "3.0.0-3090820231124001",
    "@dcloudio/uni-stacktracey": "3.0.0-3090820231124001",
    "@dcloudio/vite-plugin-uni": "3.0.0-3090820231124001",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.13.1",
    "@vue/runtime-core": "^3.2.45",
    "commitlint": "^18.4.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-plugin-prettier": "^2.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.1.0",
    "merge2": "^1.4.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-prettier": "^4.1.0",
    "typescript": "^4.9.4",
    "vite": "4.0.3",
    "vitepress": "1.0.0-rc.31",
    "vue": "^3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "vue-eslint-parser": "^9.3.2"
  }
}
